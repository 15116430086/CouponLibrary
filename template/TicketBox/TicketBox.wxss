.ticket-shop {
    width: 100%;
    height: 128rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    border-top: 1px solid #EDEDED;
    /* border-bottom: 1px solid #EDEDED; */
}

.ticketShop-one {
    text-align: center;
    font-size: 24rpx;
    font-weight: 500;
    line-height: 34rpx;
    color: rgba(153, 153, 153, 1);
    margin-left: 40rpx;
    width: 200rpx;
}
.ticketShop-one view{
  
  white-space: nowrap;
}
.tsone-one {
    font-size: 28rpx;
    font-weight: 600;
    line-height: 40rpx;
    color: rgba(226, 69, 0, 1);
    margin-bottom: 10rpx;
}

.ticketShop-two {
    margin-left: 55rpx;
    margin-right: 40rpx;
    width: calc(100% - 340rpx);
    font-size: 24rpx;
    font-weight: 500;
    line-height: 34rpx;
    color: rgba(153, 153, 153, 1);
}

.tstwo-one {
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 28rpx;
    font-weight: 600;
    line-height: 40rpx;
    color: rgba(51, 51, 51, 1);
    margin-bottom: 10rpx;
}