/* template/TicketBox/TicketBox.wxss */

.ticket-shop {
    width: 100%;
    height: 64px;
    display: flex;
    flex-direction: row;
    align-items: center;
    border-bottom: 1px solid #ededed;
}

.ticketShop-one {
    text-align: center;
    font-size: 12px;
    font-weight: 500;
    line-height: 17px;
    color: rgba(153, 153, 153, 1);
    margin-left: 20px;
}

.tsone-one {
    font-size: 14px;
    font-weight: 600;
    line-height: 20px;
    color: rgba(226, 69, 0, 1);
}

.ticketShop-two {
    margin-left: 40px;
    font-size: 12px;
    font-weight: 500;
    line-height: 17px;
    color: rgba(153, 153, 153, 1);
}

.tstwo-one {
    font-size: 14px;
    font-weight: 600;
    line-height: 20px;
    color: rgba(51, 51, 51, 1);
}

.tsone-one,
.tstwo-one {
    margin-bottom: 5px;
}